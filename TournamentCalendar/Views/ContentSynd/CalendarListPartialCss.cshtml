#PoZpU6XD {
    font-family: inherit;
    width: 100%;
}

#PoZpU6XD table {
    border: 1px solid #adb5bd;
    color: #000;
    background-color: #fff;
    font-size: 16px;
    font-size: 1rem;
    max-width: none;
    border-collapse: separate;
    border-radius: .4rem;
    line-height: 1.5;
}

#PoZpU6XD table td, #PoZpU6XD table th {
    padding: .25rem;
    text-align: left;
}

#PoZpU6XD table tbody tr td:last-child, #PoZpU6XD table thead tr th:last-child {
    text-align: center;
    color: #000;
}

#PoZpU6XD table tr td:last-child a {
    text-decoration: none;
    color: #000;
}

#PoZpU6XD table tfoot tr td {
    color: #000;
    background-color: #d3d3d3;
}

#PoZpU6XD table tfoot tr td div {
    display: inline-block;
}

#PoZpU6XD table tfoot tr td div:first-child {
    text-align: left;
    float: left;
    font-weight: bold;
}

#PoZpU6XD table tfoot tr td div:last-child {
    text-align: right;
    float: right;
}

#PoZpU6XD table tfoot tr td div a {
    color: #000;
    text-decoration: none;
}

#PoZpU6XD table tfoot tr td div a:hover {
    text-decoration: underline;
}

#PoZpU6XD table tfoot tr td a::before {
    content: "\279e\00a0";
}

#PoZpU6XD table tbody tr {
    background-color: #fff;
}

#PoZpU6XD table tbody tr:nth-child(even) {
    background-color: #ebebeb;
}

#PoZpU6XD table tbody tr:hover {
    background-color: #d3d3d3;
}

#PoZpU6XD table thead th {
    background-color: #d3d3d3;
}
